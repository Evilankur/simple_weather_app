{
  "manifest_version": 2,

  "name": "Simple Weather App By Ankur",
  "description": "Simple Weather App for chrome browser",
  "version": "2.5.1",
  "author": "Ankur",
  "browser_action": {
   "default_icon": "weather_icon.png",
   "default_popup": "popup.html"
  },
  "icons": {
    "16": "weather_icon.png",
    "48": "weather_icon.png",
    "128": "weather_icon.png"
  },
  "permissions": [
      "activeTab",
      "contextMenus",
      "storage"
    ],
  "persistent": false
}
